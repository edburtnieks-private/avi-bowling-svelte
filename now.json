{
  "version": 2,
  "name": "avi-bowling",
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    },
    {
      "src": "public/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/App.js",
      "dest": "/public/App.js"
    },
    {
      "src": "/.*",
      "dest": "server.js"
    }
  ]
}
